<nav>
    <div id="navbar">
        <div id="bayton_logo">
            {% if (footer.currentMonth() == 9) %}
            <a class="bayton-logo-light" href="/"><img src="/img/bayton_logos/bayton_rectangle_hw_light.svg" alt="main logo"/></a>
            <a class="bayton-logo-dark" href="/"><img src="/img/bayton_logos/bayton_rectangle_hw.svg" alt="main logo"/></a>            
            {% elseif (footer.currentMonth() == 11) %}
            <a class="bayton-logo-light" href="/"><img src="/img/bayton_logos/bayton_rectangle_cm_light.svg" alt="main logo"/></a>
            <a class="bayton-logo-dark" href="/"><img src="/img/bayton_logos/bayton_rectangle_cm.svg" alt="main logo"/></a>    
            {% elseif (footer.currentMonth() == 0) %}
            <a class="bayton-logo-light" href="/"><img src="/img/bayton_logos/bayton_rectangle_ny_light.svg" alt="main logo"/></a>
            <a class="bayton-logo-dark" href="/"><img src="/img/bayton_logos/bayton_rectangle_ny.svg" alt="main logo"/></a>    
            {% else %}
            <a class="bayton-logo-light" href="/"><img src="/img/bayton_logos/bayton_rectangle_light.svg" alt="main logo"/></a>
            <a class="bayton-logo-dark" href="/"><img src="/img/bayton_logos/bayton_rectangle.svg" alt="main logo"/></a>    
            {% endif %}
        </div>
        <div id="nav_links" class="mobile-hidden">
            {% for link in globalnav %}
            <div class="nav-link-individual">
                <a class="{% if link.url in page.url %} active {% endif %}" href="{{ link.url }}">{{ link.text }}</a>
            </div>
            {% endfor %} 
        </div>
        <div id="corner_search">
            <a class="{% if '/search' in page.url %} active {% endif %}" href="/search"><img src="/img/icons/magnifying-glass.svg" width="20px"></a>
        </div>
        <div id="darktoggle">
            <img src="/img/icons/clouds-moon.svg" width="25px">
        </div>
    </div>
</nav>