<nav>
    <div id="navbar">
        <div id="bayton_logo">
            {% if (footer.currentMonth() == 9) %}
            <a class="bayton-logo-light" href="/"><img src="/img/bayton_logos/bayton_rectangle_hw_light.svg" alt="halloween logo"/></a>
            <a class="bayton-logo-dark" href="/"><img src="/img/bayton_logos/bayton_rectangle_hw.svg" alt="halloween logo"/></a>            
            {% elseif (footer.currentMonth() == 11) %}
            <a class="bayton-logo-light" href="/"><img src="/img/bayton_logos/bayton_rectangle_cm_light.svg" alt="christmas logo"/></a>
            <a class="bayton-logo-dark" href="/"><img src="/img/bayton_logos/bayton_rectangle_cm.svg" alt="christmas logo"/></a>    
            {% elseif (footer.currentMonth() == 0) %}
            <a class="bayton-logo-light" href="/"><img src="/img/bayton_logos/bayton_rectangle_ny_light.svg" alt="new year logo"/></a>
            <a class="bayton-logo-dark" href="/"><img src="/img/bayton_logos/bayton_rectangle_ny.svg" alt="new year logo"/></a>    
            {% else %}
            <a class="bayton-logo-light" href="/"><img src="/img/bayton_logos/bayton_rectangle_light.svg" alt="main logo"/></a>
            <a class="bayton-logo-dark" href="/"><img src="/img/bayton_logos/bayton_rectangle.svg" alt="main logo"/></a>    
            {% endif %}
        </div>
        <div id="nav_links" class="mobile-hidden">
            {% for link in globalnav %}
            <div class="nav-link-individual">
                <a class="{% if page.url != false and link.url in page.url %} active {% endif %}" href="{{ link.url }}">{{ link.text }}</a>
            </div>
            {% endfor %} 
        </div>
        <div id="corner_search">
            <a class="{% if page.url != false and '/search' in page.url %} active {% endif %}" href="/search/"><i class="fa fa-search"></i></a>
        </div>
        <div id="darktoggle">
            <i class="fas fa-clouds-moon"></i>
        </div>
        <div id="corner_menu">
            <i class="fas fa-hamburger"></i>
        </div>
    </div>
    <div id="menu_modal">
        <button id="menu_close">&times;</button>
        <ul>
            {% for item in globalnav %}
            <li><a href="{{ item.url }}">{{ item.text }}</a></li>
            {% endfor %}
        </ul>
    </div>
</nav>
